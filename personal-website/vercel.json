{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    { 
      "handle": "filesystem" 
    },
    {
      "src": "/blog",
      "dest": "/index.html"
    },
    {
      "src": "/resume",
      "dest": "/index.html"
    },
    {
      "src": "/projects",
      "dest": "/index.html"
    },
    {
      "src": "/photography",
      "dest": "/index.html"
    }
  ]
}